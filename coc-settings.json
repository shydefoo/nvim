{
  "suggest.noselect": true,
  "suggest.floatConfig": {
      "border": true,
      "rounded": true,
  },
  "suggest.removeDuplicateItems": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "args": ["-remote=auto"],
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/", "*/**/go.mod"],
      "filetypes": ["go"],
      "disabledFeatures": [
          "workspaceFolders"
      ]
    },
    "terraform": {
        "command": "terraform-ls",
        "args": ["serve"],
        "filetypes": [
            "terraform",
            "tf"
        ],
        "initializationOptions": {},
        "settings": {}
    }
  },
  "diagnostic.displayByAle": false,
  "diagnostic.autoRefresh": true,
  "diagnostic.checkCurrentLine": false,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.enable": true,
  "diagnostic.enableMessage": "jump",
  "coc.preferences.maxFileSize": "12MB",
  "sumneko-lua.enableNvimLuaDev": true,
  "snippets.ultisnips.pythonPrompt": false,
  "snippets.extends": {
      "helm": ["kubernetes"]
  },
  "python.formatting.blackArgs": ["--line-length 120", "--skip-string-normalization"],
  "python.formatting.provider": "black",
  "python.linting.enabled": false,
  "python.linting.mypyArgs":["--implicit-optional"],
  "pyright.disableDiagnostics": true



}
